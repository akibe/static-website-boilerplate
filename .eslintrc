{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "globals": {
    "__DEV__": false,
    "alert": false,
    "console": false,
    "window": false,
    "fetch": false,
  },
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "func-names": 0,
    "max-len": 0,
    "no-use-before-define": 0,
    "semi": [2, "never"],
    "no-unused-vars": [1, { "vars": "all", "args": "none" }],
  }
}
